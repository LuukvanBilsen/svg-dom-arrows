    "lint": "eslint src/* --fix",
    "tsbuild": "typescript && typescript -m es6 --outDir lib-esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarkant/svg-dom-arrows.git"
  },
  "author": "tarkant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tarkant/svg-dom-arrows/issues"
  },
  "husky": {
